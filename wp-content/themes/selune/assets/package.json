{
	"name": "selune",
	"version": "1.0.0",
	"description": "Selune theme packages",
	"main": "main.js",
	"scripts": {
		"prod": "cross-env NODE_ENV=production webpack --mode production --progress",
		"dev": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
		"clean": "rm -rf build/*",
		"lint": "eslint src/js/*",
		"lint:fix": "eslint --fix src/js/*",
		"lint-css": "stylelint 'src/sass/**/*.scss'",
		"lint-css:fix": "npm run lint-css -- --fix",
		"precommit": "npm run lint-css:fix && npm run lint:fix && npm run prod"
	},
	"keywords": [
		"wordpress",
		"themes"
	],
	"author": "Bojan Krsmanovic",
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@babel/core": "^7.12.16",
		"@babel/preset-env": "^7.12.16",
		"@babel/preset-react": "^7.12.13",
		"@svgr/cli": "^5.5.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^3.0.1",
		"autoprefixer": "^9.8.6",
		"babel-loader": "^8.2.2",
		"clean-webpack-plugin": "^3.0.0",
		"copy-webpack-plugin": "^7.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.0.2",
		"cssnano": "^4.1.10",
		"eslint": "^7.20.0",
		"eslint-plugin-jsdoc": "^31.6.1",
		"eslint-webpack-plugin": "2",
		"file-loader": "^6.2.0",
		"lodash": "^4.17.20",
		"mini-css-extract-plugin": "^1.3.6",
		"node-sass": "^5.0.0",
		"optimize-css-assets-webpack-plugin": "^5.0.4",
		"postcss": "^8.2.6",
		"postcss-loader": "^5.0.0",
		"sass-loader": "^11.0.1",
		"sass-mq": "^5.0.1",
		"style-loader": "^2.0.0",
		"stylelint": "^13.10.0",
		"stylelint-config-wordpress": "^17.0.0",
		"stylelint-webpack-plugin": "^2.1.1",
		"uglifyjs-webpack-plugin": "^1.1.2",
		"webpack": "^5.21.2",
		"webpack-cli": "^4.5.0",
		"webpack-notifier": "^1.13.0"
	},
	"browserslist": [
		"defaults"
	],
	"_id": "selune@1.0.0",
	"dependencies": {
		"@fancyapps/ui": "^4.0.0-alpha.4"
	}
